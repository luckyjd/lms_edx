#!/opt/lms_edx/edx-ficus.3-3/apps/edx/venvs/edxapp/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'XModule','console_scripts','xmodule_assets'
__requires__ = 'XModule'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('XModule', 'console_scripts', 'xmodule_assets')()
    )
